<div class="container">

<h1>Questions</h1>
<% @questions.each do |question| %>
  <h2><%= question.title %></h2>
  <%= l question.created_at, format: :long %>
  <p>
    <%= question.body %>
    <br>
    <%= link_to 'New', new_question_path(question) %> |
    <%= link_to 'Show', question_path(question) %> |
    <%= link_to 'Edit', edit_question_path(question) %> |
  </p>
<% end %>
</div>